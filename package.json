{
  "name": "screader",
  "version": "1.3.5",
  "description": "screenshot ocr/reader to clipboard",
  "main": "./bin/screader",
  "type": "module",
  "bin": {
    "screader": "./bin/screader"
  },
  "engines": {
    "node": ">=18.20.3",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "start": "./bin/screader",
    "build": "esbuild ./bin/screader --bundle --platform=node --outfile=dist/screader.js && pkg --out-path ./dist ./dist/screader.js",
    "lint": "standard ."
  },
  "homepage": "https://github.com/natzcam/screader",
  "repository": "https://github.com/natzcam/screader",
  "bugs": "https://github.com/natzcam/screader/issues",
  "keywords": [
    "screenshot",
    "ocr",
    "utility",
    "screen reader",
    "screenshot reader",
    "clipboard"
  ],
  "author": "Nathaniel Camomot",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "0.21.4",
    "pkg": "^5.8.1",
    "standard": "^17.1.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.5",
    "chokidar": "^3.6.0",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "conf": "^12.0.0",
    "tesseract.js": "^5.1.0"
  }
}
